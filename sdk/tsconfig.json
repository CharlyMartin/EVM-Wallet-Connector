{
    "extends": "@fuels/ts-config/base.json",
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["DOM", "ESNext"],
        "module": "CommonJS",
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "declaration": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "outDir": "./dist"
    },
    "exclude": [
        "./dist/**/*"
    ]
}